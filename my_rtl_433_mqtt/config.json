{
  "name": "Auto Discover rtl_433 MQTT",
  "version": "0.0.14",
  "slug": "auto_rtl433mqtt",
  "description": "Automatic discovery of rtl_433 device information via MQTT",
  "url": "https://github.com/sebmorio/homeassistant_addons",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": true,
  "options": {
    "mqtt_host": "homeassistant.local",
    "mqtt_port": 1883,
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_retain": true,
    "rtl_topic": "rtl_433/+/events",
    "device_topic_suffix": "",
    "discovery_prefix": "homeassistant",
    "discovery_interval": 300,
    "force_update": false,
    "log_level": "default"
  },
  "schema": {
    "mqtt_host": "str?",
    "mqtt_port": "int?",
    "mqtt_user": "str?",
    "mqtt_password": "password",
    "mqtt_retain": "bool",
    "rtl_topic": "str",
    "device_topic_suffix": "str?",
    "discovery_prefix": "str",
    "discovery_interval": "int",
    "force_update": "bool",
    "log_level": "list(default|quiet|debug)"
  },
  "services": ["mqtt:want"]
}
