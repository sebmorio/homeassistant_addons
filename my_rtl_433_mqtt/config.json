
  "name": "My Custom rtl433 mqtt",
  "version": "0.1.3",
  "slug": "MyCustom_rtl433_mqtt",
  "description": "Receive data from SDR dongle rtl_433 and push in MQTT. (Special feat For My WS6262)",
  "url": "https://github.com/sebmorio/homeassistant_addons/",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "map": ["config:rw"],
  "services": ["mqtt:want"],
  "usb": true,
  "udev": true,
  "options": {
    "retain": true,
    "mqtt_host": "homeassistant.local",
    "mqtt_port": "1883",
    "mqtt_user": "",
    "mqtt_password": ""
  },
  "schema": {
    "retain": "bool",
    "mqtt_host": "str",
    "mqtt_port": "int",
    "mqtt_user": "str",
    "mqtt_password": "password"
  }
}
